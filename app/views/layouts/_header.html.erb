<% if logged_in? %>
  <strong><%= current_user.username %></strong> is logged in<br>
  <%= link_to "Create event", new_event_path %>
  <%= link_to "My events", user_path(current_user.id) %>
  <%= link_to "Show events", events_path %>
  <%= link_to "Log out", logout_path, method: :delete %><br></br>
<% else %>
  <%= link_to "Login", login_path %>
  <%= link_to "Sign up", signup_path %>
<% end %>