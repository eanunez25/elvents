<%= provide(:title, @user.username) %>

<%= @user.username%><br></br>

<% if @user.created_events.any? %>
  Created events: <%= @user.created_events.count %>
  <ol>
    <% @created_events.each do |event| %>
      <li><a href="/events/<%= event.id %>"><%= event.description %></a></li>
    <% end %>
  </ol>
<% else %>
  <%= @user.username %> has no created events.
<% end %>
<br></br>

<% if @user.attended_events.any? %>
  Attended events: <%= @user.attended_events.count %>
  <% @attended_events.each do |event| %>
    <li><a href="/events/<%= event.id %>"><%= event.description %></a></li>
  <% end %>
<% else %>

<% end %>

